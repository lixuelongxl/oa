<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.core136.mapper.sys.SysDbSourceMapper">
<resultMap id="SysDbSource" type="com.core136.bean.sys.SysDbSource" >
    <id column="ID" property="id" jdbcType="BIGINT" />
    <result column="DB_SOURCE_ID" property="dbSourceId" jdbcType="VARCHAR" />
    <result column="DB_SOURCE_NAME" property="dbSourceName" jdbcType="VARCHAR" />
    <result column="SORT_NO" property="sortNo" jdbcType="INTEGER" />
    <result column="DB_SOURCE_TYPE" property="dbSourceType" jdbcType="VARCHAR" />
    <result column="DB_LINK" property="dbLink" jdbcType="VARCHAR" />
    <result column="DB_USER_NAME" property="dbUserName" jdbcType="VARCHAR" />
    <result column="DB_PASS_WORD" property="dbPassWord" jdbcType="VARCHAR" />
    <result column="CREATE_USER" property="createUser" jdbcType="VARCHAR" />
    <result column="CREATE_TIME" property="createTime" jdbcType="VARCHAR" />
    <result column="ORG_ID" property="orgId" jdbcType="VARCHAR" />
  </resultMap>
<select id="getDbSourceList" resultType="java.util.HashMap">
SELECT DB_SOURCE_ID AS dbSourceId,DB_SOURCE_NAME AS dbSourceName FROM SYS_DB_SOURCE WHERE ORG_ID=#{orgId} ORDER BY SORT_NO ASC;
</select>

</mapper>

